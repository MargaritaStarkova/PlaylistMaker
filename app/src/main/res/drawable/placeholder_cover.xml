<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="312dp"
        android:height="312dp"
        android:gravity="center">
        <shape android:shape="rectangle">
            <stroke
                android:width="1dp"
                android:color="?attr/yp_text_grey_color"
                android:dashWidth="30dp"
                android:dashGap="30dp" />
            <corners android:radius="@dimen/corner_radius_8dp" />
        </shape>
    </item>

    <item
        android:drawable="@drawable/ic_add_photo"
        android:gravity="center" />

</layer-list>